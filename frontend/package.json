{
  "name": "basenaturaliste",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run lint && tsc",
    "build:watch": "tsc -w",
    "build:e2e": "tsc -p e2e/",
    "copy:statics": "cpx \"src/**/*.{html,css,png,jpg,ico}\" \"dist/\" --watch",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "cpx \"src/systemjs*.js\" \"dist/\" && yarn run build",
    "start": "concurrently  \"yarn run copy:statics\" \"yarn run build:watch\" \"yarn run serve\" ",
    "pree2e": "yarn run build:e2e",
    "e2e": "concurrently \"yarn run serve:e2e\" \"yarn run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "yarn run build",
    "test": "concurrently \"yarn run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "yarn run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint -p . -t stylish"
  },
  "keywords": [],
  "author": "Camille Carrier",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~4.0.0",
    "@angular/compiler": "~4.0.0",
    "@angular/core": "~4.0.0",
    "@angular/forms": "~4.0.0",
    "@angular/http": "~4.0.0",
    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@angular/router": "~4.0.0",
    "angular-in-memory-web-api": "~0.3.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.0.1",
    "systemjs": "0.19.40",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.36",
    "@types/node": "^6.0.46",
    "canonical-path": "0.0.2",
    "concurrently": "^3.2.0",
    "cpx": "^1.5.0",
    "jasmine-core": "~2.4.1",
    "karma": "^1.7",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "protractor": "~4.0.14",
    "rimraf": "2",
    "tslint": "*",
    "typescript": "2"
  },
  "repository": {}
}